{
  "name": "ai-yuugen-platform",
  "version": "1.0.0",
  "description": "Open-source Ad Yuugen mechanism for AI interfaces",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "nx run-many -t build",
    "build:affected": "nx affected -t build",
    "build:production": "nx run-many -t build --configuration=production",
    "test": "nx run-many -t test",
    "test:affected": "nx affected -t test",
    "test:e2e": "cd e2e && npm test",
    "test:e2e:ui": "cd e2e && npm run test:ui",
    "test:e2e:headed": "cd e2e && npm run test:headed",
    "test:e2e:setup": "cd e2e && npm run setup",
    "test:all": "npm run lint && npm run test && npm run test:e2e",
    "lint": "nx run-many -t lint",
    "lint:affected": "nx affected -t lint",
    "lint:fix": "nx run-many -t lint --fix",
    "dev": "nx run-many -t dev --parallel=3",
    "dev:server": "nx run server:dev",
    "dev:portal": "nx run developer-portal:dev",
    "dev:storybook": "nx run storybook:dev",
    "dev:test-app": "cd e2e && npm run dev:test-app",
    "clean": "nx run-many -t clean",
    "build:docs": "node scripts/build-docs.js",
    "docs:serve": "npx http-server docs -p 8080 -o",
    "graph": "nx graph",
    "reset": "nx reset",
    "poc:adcp": "cd poc/adcp-integration && npm start",
    "prepare-release": "./scripts/prepare-release.sh",
    "publish:packages": "./scripts/publish-packages.sh",
    "publish:dry-run": "./scripts/publish-packages.sh --dry-run",
    "docker:build": "docker-compose -f docker/docker-compose.yml build",
    "docker:up": "docker-compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker-compose -f docker/docker-compose.yml down",
    "docker:logs": "docker-compose -f docker/docker-compose.yml logs -f",
    "security:audit": "npm audit --audit-level=moderate",
    "version:patch": "npm version patch --workspaces",
    "version:minor": "npm version minor --workspaces",
    "version:major": "npm version major --workspaces"
  },
