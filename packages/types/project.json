{
  "name": "types",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/types/src",
  "projectType": "library",
  "tags": ["scope:shared", "type:types"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc",
        "cwd": "packages/types"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --watch",
        "cwd": "packages/types"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest",
        "cwd": "packages/types"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint src --ext .ts",
        "cwd": "packages/types"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rm -rf dist",
        "cwd": "packages/types"
      }
    }
  }
}
