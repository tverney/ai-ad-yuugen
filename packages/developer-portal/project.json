{
  "name": "developer-portal",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/developer-portal/src",
  "projectType": "application",
  "tags": ["scope:client", "type:app"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite build",
        "cwd": "packages/developer-portal"
      },
      "dependsOn": ["^build"]
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite",
        "cwd": "packages/developer-portal"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite preview",
        "cwd": "packages/developer-portal"
      },
      "dependsOn": ["build"]
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest",
        "cwd": "packages/developer-portal"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint src --ext .ts,.tsx",
        "cwd": "packages/developer-portal"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rm -rf dist",
        "cwd": "packages/developer-portal"
      }
    }
  }
}
