{
  "name": "ai-yuugen-sdk-e2e-tests",
  "version": "1.0.0",
  "description": "End-to-end tests for AI Ad Yuugen SDK",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "test:report": "playwright show-report",
    "test:install": "playwright install",
    "dev:test-app": "cd test-app && python3 -m http.server 3000",
    "dev:server": "cd ../packages/server && npm run dev",
    "setup": "npm install && playwright install",
    "validate": "node scripts/validate-setup.js",
    "pretest": "node scripts/validate-setup.js"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}